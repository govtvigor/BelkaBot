{
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/create-invoice.ts",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/getReferralLink.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/index",
      "dest": "/server/index.ts"
    },
    {
      "src": "/api/create-invoice",
      "dest": "/server/api/create-invoice.ts"
    },
    {
      "src": "/api/getReferralLink",
      "dest": "/server/api/getReferralLink.ts"
    }
  ]
}
