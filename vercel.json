{
    "builds": [
      {
        "src": "src/client/index.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "src/server/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/src/server/index.ts" },
      { "src": "/(.*)", "dest": "/src/client/index.js" }
    ]
  }
  